	"Require assets script":{
		"prefix": "reqAssets",
		"body": [
			"var assets = require('*/cartridge/scripts/assets.js');"
		],
		"description": "Require assets script"
	},
	"Require basic PD component assets":{
		"prefix": "pdAssets",
		"body": [
			"<isscript>",
				"\tvar assets = require('*/cartridge/scripts/assets.js');",
				"\tassets.addCss('/css/experience/components/commerceAssets/${1:$TM_FILENAME_BASE}.css');",
				"\tassets.addJs('/js/${1:$TM_FILENAME_BASE}.js');",
			"</isscript>"
		],
		"description": "Include JS and css assets in Page designer components from commerce assets folder"
	},
	"Require basic PD Layout assets":{
		"prefix": "pdlAssets",
